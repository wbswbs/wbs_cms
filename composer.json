{
  "name": "wbswbs/wbs_cms",
  "description": "Einfaches CMS auf Smarty Template Basis",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Wolfgang Blessen",
      "email": "wolfgang@blessen.de"
    }
  ],
  "config": {
    "platform": {
      "php": "8.2"
    },
    "allow-plugins": {
      "third-party/required-plugin": true,
      "unnecessary/plugin": false
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "require": {
    "cboden/ratchet": "0.4.*",
    "ext-curl": "*",
    "ext-fileinfo": "*",
    "ext-ftp": "*",
    "ext-gettext": "*",
    "ext-imagick": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "ext-pdo": "*",
    "ext-apcu": "*",
    "ext-pdo_mysql": "*",
    "ext-mbstring": "*",
    "ext-tidy": "*",
    "gedmo/doctrine-extensions": "3.14.*",
    "php": "8.2.*",
    "setasign/fpdf": "1.8.*",
    "symfony/routing": "6.4.*",
    "symfony/yaml": "7.0.*",
    "textalk/websocket": "1.6.*",
    "wbswbs/wbsfw": "1.2.*",
    "wrench/wrench": "2.0.*"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:wbswbs/wbsfw.git"
    }
  ]
}
